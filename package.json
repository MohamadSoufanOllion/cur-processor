{
  "name": "cur-processor",
  "version": "0.1.0",
  "bin": {
    "cur-processor": "bin/cur-processor.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "show-template": "cdk bootstrap --show-template > bootstrap-template.yaml --qualifier alo --profile hearst-quicksight",
    "bootstrap-template": "cdk bootstrap --template bootstrap-template.yaml --toolkit-stack-name alo-CDKToolkit --profile hearst-quicksight",
    "local-synth": "npm run build && cdk synth --no-staging > template.yaml --profile hearst-quicksight",
    "deploy": "cdk deploy --profile hearst-quicksight",
    "local-synth-ci": "npm run build && cdk synth --no-staging > template.yaml --profile hearst-cicd",
    "deploy-ci": "cdk deploy --profile hearst-cicd --toolkit-stack-name alo-CDKToolkit",
    "deploy-ci-role": "cdk deploy --profile hearst-cicd --toolkit-stack-name alo-CDKToolkit -r arn:aws:iam::382938011234:role/alo-cdk-deploy-role-382938011234-us-east-1",
    "local-synth-joey": "npm run build && cdk synth --no-staging > template.yaml --profile joey",
    "deploy-joey": "cdk deploy --profile joey",
    "local-synth-joey-cross": "npm run build && cdk synth --no-staging > template.yaml --profile joey-cross",
    "deploy-joey-cross": "cdk deploy --profile joey-cross"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.10",
    "@types/nodemailer": "^6.4.16",
    "@types/source-map-support": "^0.5.10",
    "aws-cdk": "^2.160.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-bcm-data-exports": "^3.666.0",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/client-ses": "^3.658.1",
    "@aws-sdk/client-sts": "^3.670.0",
    "aws-cdk-lib": "^2.160.0",
    "axios": "^1.7.7",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.5",
    "fs": "^0.0.1-security",
    "nodemailer": "^6.9.15",
    "source-map-support": "^0.5.21"
  }
}
